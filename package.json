{
  "name": "sunruse-influx",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc -p .",
    "build-watch": "tsc -w -p .",
    "build-test-watch": "npm-run-all --parallel build-watch test-watch",
    "install-build-test-watch": "npm install && npm run-script build-test-watch",
    "test-watch": "nodemon -e js --watch spec --watch dist --exec \"npm test\"",
    "test": "jasmine JASMINE_CONFIG_PATH=spec/jasmine.json"
  },
  "devDependencies": {
    "@types/node": "^7.0.13",
    "jasmine": "^2.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "rewire": "^2.5.2",
    "typescript": "^2.2.2"
  }
}
